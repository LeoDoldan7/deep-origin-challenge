{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "typescript"
  ],
  "eslint.format.enable": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#8ce39e",
    "activityBar.background": "#8ce39e",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#937fe0",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#8ce39e",
    "statusBar.background": "#63d97c",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#3acf5a",
    "statusBarItem.remoteBackground": "#63d97c",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#63d97c",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#63d97c99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#63d97c",
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "server",
          "commands": ["docker-compose up -d", "npm run dev"]
        }       
      ]
    },
   
  ],
  "restoreTerminals.runOnStartup": true
}
